{
  "name": "react-starter",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.1.4",
  "author": "Donny Wang <donny526@outlook.com>",
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\"",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "postinstall": "simple-git-hooks",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "ahooks": "^3.8.0",
    "antd": "^5.17.4",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "echarts": "^5.5.0",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@coderwyd/eslint-config": "^2.4.2",
    "@eslint-react/eslint-plugin": "^1.5.14",
    "@types/node": "^20.12.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@unocss/eslint-plugin": "^0.60.3",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.3.0",
    "eslint-plugin-react-hooks": "5.1.0-rc-f994737d14-20240522",
    "eslint-plugin-react-refresh": "^0.4.7",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.4",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unocss": "^0.60.3",
    "vite": "^5.2.12",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
